{
  "bindings": [
    {
      "type": "httpTrigger",
      "authLevel": "anonymous",
      "direction": "in",
      "name": "req",
      "methods": ["post"],
      "route": "terminate/{instanceId}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "durableClient",
      "direction": "in",
      "name": "client"
    }
  ]
}
