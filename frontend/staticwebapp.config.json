{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.png", "/*.ico"]
  },

  "routes": [
    {
      "route": "/account/login",
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/account/*",
      "allowedRoles": ["authenticated"]
    }
  ]
}
